{
	"extends": [
		"tslint:latest",
		"tslint-config-prettier"
	],
	"rules": {
		"interface-name": false,
		"object-literal-sort-keys": false,
		"no-implicit-dependencies": [true, ["ts-jest"]],
		"variable-name": [true, "ban-keywords", "check-format", "allow-leading-underscore", "require-const-for-all-caps"]
	}
}
